{
    "name": "Listener",
    "description": "hello, please speak to me",
    "version": "0.1",
    "manifest_version": 2,
    "author": "Jake Wright and Jacob Beckerman",

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["libraries/p5.js", "libraries/p5.dom.js", "sketch.js"]
    }],

    "browser_action": {
        "default_title": "I am listening ::)",
        "default_popup": "popup.html"
    },

    

    "permissions": ["tabs"]
}