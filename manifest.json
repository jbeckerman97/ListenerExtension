{
    "name": "SpeakBitch",
    "description": "Hey bitch, speak",
    "version": "0.1",
    "manifest_version": 2,
    "author": "Jake Wright and Jacob Beckerman",

    "browser_action": {
        "default_icon": "tab_icon_1.jpg",
        "default_title": "bitch pls speak",
        "default_popup": "popup.html"
    },

    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["scripts/content.js"]
    }],

    "permissions": ["tabs"]
}